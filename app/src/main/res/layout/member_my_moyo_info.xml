<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout android:id="@+id/mymoyo_map_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1.5"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:paddingHorizontal="20dp"
        android:paddingVertical="20dp"
        android:background="@color/white">

        <TextView
            android:id="@+id/mymoyoinfo_moyoName"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="5dp"
            android:text="모여제목이올자리입니다"
            android:textColor="@color/black"
            android:textSize="9pt"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/mymoyoinfo_moyoAddr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:paddingVertical="5dp"
            android:layout_marginLeft="10dp"
            android:text="서울특별시 금천구 시흥대로149가길 32"
            android:textColor="@color/black"
            android:textSize="7pt" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:id="@+id/mymoyoinfo_moyoStart"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingVertical="5dp"
                android:text="2020-00-00"
                android:textColor="@color/black"
                android:textSize="7pt" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="82dp"
                android:layout_marginLeft="82dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="부터"
                android:textColor="@color/black"
                android:textSize="7pt" />

            <TextView
                android:id="@+id/mymoyoinfo_moyoEnd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="115dp"
                android:layout_marginLeft="115dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="2021-99-99"
                android:textColor="@color/black"
                android:textSize="7pt" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="198dp"
                android:layout_marginLeft="198dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="까지 모여!"
                android:textColor="@color/black"
                android:textSize="7pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingVertical="5dp"
                android:text="모여 DAY : "
                android:textColor="@color/black"
                android:textSize="7pt" />

            <TextView
                android:id="@+id/mymoyoinfo_moyoDday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="70dp"
                android:layout_marginLeft="70dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="2020-00-00"
                android:textColor="@color/black"
                android:textSize="7pt" />

        </RelativeLayout>

        <TextView
            android:id="@+id/mymoyoinfo_moyoDesc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingVertical="5dp"
            android:layout_marginLeft="10dp"
            android:text="모여 설명설명"
            android:textColor="@color/black" />

        <View
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginVertical="15dp"
            android:alpha="0.5"
            android:background="@color/maincolor" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="5dp"
            android:text="모여 신청 정보"
            android:textColor="@color/black"
            android:textSize="8pt"
            android:textStyle="bold" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingVertical="5dp"
                android:text="신청자 이름  :  "
                android:textColor="@color/black"
                android:textSize="6pt" />

            <TextView
                android:id="@+id/mymoyoinfo_moyoUseName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="78dp"
                android:layout_marginLeft="78dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="강혜수"
                android:textColor="@color/black"
                android:textSize="6pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingVertical="5dp"
                android:text="신청자 전화번호  :  "
                android:textColor="@color/black"
                android:textSize="6pt" />

            <TextView
                android:id="@+id/mymoyoinfo_moyoUsePhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="101dp"
                android:layout_marginLeft="101dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="010-0000-0000"
                android:textColor="@color/black"
                android:textSize="6pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingVertical="5dp"
                android:text="신청자 이메일  :  "
                android:textColor="@color/black"
                android:textSize="6pt" />

            <TextView
                android:id="@+id/mymoyoinfo_moyoUseEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="90dp"
                android:layout_marginLeft="90dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="aaaaaa@gmail.com"
                android:textColor="@color/black"
                android:textSize="6pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingVertical="5dp"
                android:text="방문 예정 시간  :  "
                android:textColor="@color/black"
                android:textSize="6pt" />

            <TextView
                android:id="@+id/mymoyoinfo_moyoUseTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="90dp"
                android:layout_marginLeft="90dp"
                android:layout_marginBottom="0dp"
                android:paddingVertical="5dp"
                android:text="10:00"
                android:textColor="@color/black"
                android:textSize="6pt" />

        </RelativeLayout>

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/trackingMyMoyoBus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="-18dp"
            android:layout_marginRight="15dp"
            android:backgroundTint="@color/maincolor"
            android:clickable="true"
            android:elevation="20dp"
            android:src="@drawable/busiconwhite" />

    </LinearLayout>


</LinearLayout>